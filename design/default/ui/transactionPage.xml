          <label name="fromValue" x="65" y="1" w="245" h="23" v-data:text="{trans.creditAccount}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view5" h="24" style:normal:border_color="#00000000" x="0" y="196" w="320">
          <label name="from" x="10" y="0" w="46" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Status"/>
          <label name="fromValue" x="56" y="-2" w="254" h="25" v-data:text="{trans.trnStatus}" style:normal:text_align_h="right" style:normal:font_size="12"/>
        </view>
        <view name="view3" h="24" style:normal:border_color="#00000000" x="0" y="162" w="320">
          <label name="from" x="10" y="0" w="82" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Bank"/>
          <label name="fromValue" x="65" y="1" w="245" h="23" v-data:text="{trans.bankName}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <button v-on:click="{printNext, args=1}" name="button4" x="10" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#7B7B7B" style:normal:text_color="#FFFFFF" style:normal:border_color="#00000000" text="Merchant Copy"/>
        <button v-on:click="{onPrint}" name="button4" x="184" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#00000000" style:normal:text_color="#000000" style:normal:border_color="#7B7B7B" text="Customer Copy"/>
        <view name="view12" h="35" style:normal:border_color="#7E7E7E" x="0" y="230" w="320" style:normal:border="top" style:normal:border_width="1">
          <label name="from" x="10" y="0" w="300" h="19" style:normal:font_size="14" style:normal:text_align_h="center" text="Transaction I.D"/>
          <label name="fromValue" x="0" y="10" w="320" h="25" v-data:text="{trans.trnReference}" style:normal:text_align_h="center" style:normal:font_size="12"/>
        </view>
      </view>
      <view v-if="{trans.trnService == CARD_COLLECTION}" name="transaction_summary_card" h="357" style:normal:bg_color="#FFFFFF" style:normal:round_radius="5">
        <view v-if="{trans.drCr == CR}" w="320" h="35">
          <label name="tansaction_amount" x="85" y="0" w="42" h="25" style:normal:font_size="24" style:normal:text_color="#00B050" text="credit"/>
          <label name="tansaction_amount" x="180" y="1" w="65" h="25" style:normal:font_size="24" v-data:text="{amount}" style:normal:text_color="#00B050"/>
        </view>
        <view v-if="{trans.drCr == DR}" w="320" h="25">
          <label name="tansaction_amount" x="85" y="0" w="42" h="25" style:normal:font_size="24" style:normal:text_color="#C00000" text="debit"/>
          <label name="tansaction_amount" x="180" y="1" w="69" h="25" style:normal:font_size="24" v-data:text="{amount}" style:normal:text_color="#C00000"/>
        </view>
        <view name="view9" x="0" y="26" w="320" h="24" style:normal:border_color="#00000000">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Date:"/>
          <label name="fromValue" x="91" y="1" w="219" h="23" v-data:text="{trans.date}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view8" h="24" style:normal:border_color="#00000000" x="0" y="60" w="320">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Card:"/>
          <label name="fromValue" x="101" y="0" w="209" h="23" v-data:text="{extraData.card}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view7" h="24" style:normal:border_color="#00000000" x="0" y="94" w="320">
