          <label name="fromValue" x="81" y="1" w="229" h="23" v-data:text="{extraData.appLab}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view6" h="24" style:normal:border_color="#00000000" x="0" y="128" w="320">
          <label name="from" x="10" y="1" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Stan"/>
          <label name="fromValue" x="65" y="1" w="245" h="23" v-data:text="{extraData.stan}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view5" h="24" style:normal:border_color="#00000000" x="0" y="196" w="320">
          <label name="from" x="10" y="0" w="46" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Status"/>
          <label name="fromValue" x="56" y="-2" w="254" h="25" v-data:text="{responseMessage}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view3" h="24" style:normal:border_color="#00000000" x="0" y="162" w="320">
          <label name="from" x="10" y="0" w="82" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Rrn"/>
          <label name="fromValue" x="65" y="1" w="245" h="23" v-data:text="{extraData.rrn}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <button v-on:click="{printNext, args=1}" name="button4" x="10" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#7B7B7B" style:normal:text_color="#FFFFFF" style:normal:border_color="#00000000" text="Merchant Copy"/>
        <button v-on:click="{onPrint}" name="button4" x="184" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#00000000" style:normal:text_color="#000000" style:normal:border_color="#7B7B7B" text="Customer Copy"/>
