<?xml ruler_x="49"?>
<window v-model="transactionPage" name="transactionPage" v-on:key_down:NETEVENT="{initHTTPCB}" v-on:key_down:cancel="{onKeyDown, Args=cancel}" v-on:key_down:return="{onKeyDown, Args=return}" floating="false" style="default" style:normal:border="none">
  <label name="label" x="102" y="0" w="117" h="25" style:normal:font_size="16" style:normal:text_align_h="center" text="Transaction History"/>
  <view name="view3" h="35" x="0" y="25" w="320" style:normal:bg_color="#FFFFFF">
    <view v-if="{totalPageNum > 1}" style:normal:bg_color="#FFFFFF" h="35" name="inner_view3" w="171">
      <label name="label4" h="28" x="65" y="4" w="38" v-data:text="{totalPage}" style:normal:font_size="14"/>
      <button v-if="{currentPage > 1}" v-on:click="prevPage" name="prev" h="28" x="10" y="4" w="45" style:normal:round_radius="6" style:normal:font_size="14" style:normal:bg_color="#00000000" style:disable:text_color="#A5A5A5" text="Prev"/>
      <button v-if="{currentPage < totalPageNum}" v-on:click="nextPage" name="next" h="28" style:normal:round_radius="6" style:normal:font_size="14" style:normal:bg_color="#00000000" x="123" y="4" w="45" style:disable:text_color="#A5A5A5" text="Next"/>
    </view>
    <button name="refresh_btn" v-on:click="{refreshList}" x="181" y="3" w="54" h="28" style:normal:round_radius="6" style:normal:font_size="16" style:pressed:font_size="16" style:pressed:round_radius="6" style:over:font_size="16" style:over:round_radius="6" style:normal:bg_color="#00000000" text="Reload"/>
    <button name="filter_btn" v-on:click="{toggleFilter}" x="245" y="4" w="66" h="27" style:normal:round_radius="6" style:normal:spacer="2" style:normal:margin="0" style:normal:bg_color="#848484" style:normal:font_size="16" style:pressed:round_radius="6" style:over:round_radius="6" style:over:font_size="16" style:pressed:font_size="16" style:normal:text_color="#FFFFFF" text="Filter"/>
  </view>
  <view v-if="{!isTransactions}" name="no_transaction" x="0" y="60" w="320" h="152" style:normal:bg_image="" style:normal:bg_color="#FFFFFF" disg:hide="true">
    <image name="image2" x="104" y="0" w="100" h="100" draw_type="icon" image="no_transactions"/>
  </view>
  <list_view v-if="{isTransactions}" name="list_view" x="0" y="60" w="320" h="150" default_item_height="60">
    <scroll_view name="scroll_view" x="0" y="0" w="320" h="150" yslidable="true" with_focus_state="true" move_to_page="true" style:normal:bg_color="#FFFFFF">
      <list_item v-for="{transactions}" v-on:click="{getTrans,Args=fscript?amount=item.trnAmount&amp;&amp;date=item.trnCreatedAt&amp;&amp;creditAccountName=item.trnCrAccountName&amp;&amp;creditAccount=item.trnCrAccountNumber&amp;&amp;debitAccountName=item.trnDrAccountName&amp;&amp;debitAccountNumber=item.trnDrAccountNumber&amp;&amp;bankName=item.trnCrInstitutionName&amp;&amp;reference=item.trnReference&amp;&amp;narration=item.trnNarration&amp;&amp;responseCode=item.trnResponseCode&amp;&amp;status=item.trnStatus&amp;&amp;drCr=item.trnDrCr&amp;&amp;extraData=item.trnExtraData&amp;&amp;trnService=item.trnService&amp;&amp;responseMessage=item.trnResponseMessage&amp;}" name="list_item" h="65" focusable="true" with_focus_state="true" style:normal:bg_color="#FFFFFF" style:normal:border="all" style:pressed:border="none" style:over:border="none" style:focused:border="none" style:over:bg_color="#8EAAD778" style:pressed:bg_color="#8EAAD778" style:focused:bg_color="#8EAAD778" style:normal:margin_bottom="0">
        <view name="view" h="64" style:normal:bg_color="#FFFFFF" x="0" y="1" w="320">
          <label v-if="{item.trnStatus == PENDING}" name="status__" h="20" x="263" y="41" w="39" style:normal:text_color="#FFC000" style:normal:font_size="14" text="PENDING"/>
          <label v-if="{item.trnStatus == FAILED}" name="status_" h="20" x="258" y="41" w="49" style:normal:text_color="#C00000" style:normal:font_size="14" text="DECLINED"/>
          <label v-if="{item.trnStatus == ACTIVE || status == SUCCESS}" name="status" h="20" x="258" y="41" w="52" style:normal:text_color="#375622" style:normal:font_size="14" text="APPROVED"/>
          <label v-if="{item.trnDrCr == DR}" name="debit" h="28" v-data:text="{item.trnAmount}" x="258" y="10" w="52" style:normal:text_color="#FF0000" style:normal:font_size="18" style:normal:font_name="default" style:normal:text_align_h="left"/>
          <label v-if="{item.trnDrCr == CR}" name="credit" h="28" x="258" y="10" w="52" v-data:text="{item.trnAmount}" style:normal:text_color="#37563D" style:normal:font_size="18" style:normal:text_align_h="right"/>
          <label name="trans_narration" h="20" x="10" y="34" w="80" style:normal:text_align_h="left" v-data:text="{item.trnCreatedAt}" style:normal:font_size="12"/>
          <label v-if="{item.trnService == 'CARD_COLLECTION'}" name="trans_narration" h="20" x="10" y="10" w="164" style:normal:text_align_h="left" v-data:text="{item.trnNarration}" style:normal:font_size="13"/>
        </view>
      </list_item>
    </scroll_view>
    <scroll_bar_m name="scroll_bar" x="310" y="0" w="10" h="170" style:normal:round_radius="5" style:normal:border_width="0" style:normal:bg_color="#F4F5F8"/>
  </list_view>
  <view v-if="loading" name="view1" x="0" y="0" w="320" h="210" style:normal:bg_color="#FFFFFF" disg:hide="true">
    <label name="label1" x="77" y="111" w="160" h="28" style:normal:font_size="18" v-data:text="{showTip}"/>
    <image name="image" x="135" y="61" w="44" h="40" draw_type="icon" image="nodata"/>
  </view>
  <view v-if="{filterOn}" name="filter_modal" x="0" y="0" w="320" h="210" style:normal:bg_color="#000000A7" floating="true" focusable="true" with_focus_state="true" disg:hide="true">
    <view name="view5" x="0" y="0" w="320" h="210" style:normal:bg_color="#FFFFFF" style:normal:round_radius="0">
      <label name="label3" x="10" y="10" w="116" h="28" style:normal:text_align_h="left" text="Filter Transactions:"/>
      <button name="yesterday" v-on:click="{getYesterdayDate}" x="39" y="63" w="228" h="40" style:normal:round_radius="6" style:normal:bg_color="#00000000" style:pressed:round_radius="6" style:over:round_radius="6" text="Yesterday"/>
      <button v-on:click="{toggleCustomFilter}" name="custom" x="39" y="135" w="228" h="40" style:normal:round_radius="6" style:normal:bg_color="#00000000" style:pressed:round_radius="6" style:over:round_radius="6" text="Custom Date"/>
      <button v-on:click="{toggleFilter}" name="close_btn" x="276" y="10" w="34" h="30" style:normal:bg_image_draw_type="icon" style:normal:bg_image="end_" style:normal:round_radius="6" style:normal:border_color="#00000000" style:normal:font_size="24" style:normal:bg_color="#C00000" style:pressed:round_radius="6" style:pressed:border_color="#00000000" style:pressed:bg_color="#C00000" style:pressed:bg_image_draw_type="icon" style:pressed:bg_image="end_" style:over:border_color="#00000000" style:over:round_radius="6" style:over:bg_color="#C00000" style:over:bg_image_draw_type="icon" style:over:bg_image="end_"/>
    </view>
    <view v-if="{customDate}" name="view5" x="0" y="0" w="320" h="210" style:normal:bg_color="#FFFFFF" style:normal:round_radius="0" disg:hide="true">
      <view name="startDate" x="22" y="48" w="266" h="64">
        <label name="label3" x="0" y="0" w="129" h="28" style:normal:text_align_h="left" text="Custom Date"/>
        <spin_box min="0" max="1024" step="1" v-data:text="{startDate}" name="spin_box" x="0" y="28" w="266" h="37" tips="Please enter your value" input_type="date" auto_fix="true" style:empty:text_color="#000000a7" style:empty:font_name="ap" cancelable="true" style:normal:border_color="#7E7E7E" style:normal:bg_color="#FFFFFF" style:normal:round_radius="4"/>
      </view>
      <button v-on:click="{logCustomDate}" name="custom" x="22" y="122" w="107" h="40" style:normal:round_radius="6" style:normal:bg_color="#00000000" style:pressed:round_radius="6" style:over:round_radius="6" text="Filter"/>
      <button v-on:click="{toggleCustomFilter}" name="close_btn" x="276" y="10" w="34" h="29" style:normal:bg_image_draw_type="icon" style:normal:bg_image="end_" style:normal:round_radius="6" style:normal:border_color="#00000000" style:normal:font_size="24" style:normal:bg_color="#C00000" style:pressed:round_radius="6" style:pressed:bg_image_draw_type="icon" style:pressed:bg_image="end_" style:pressed:bg_color="#C00000" style:over:border_color="#00000000" style:over:round_radius="6" style:over:bg_color="#C00000" style:over:bg_image_draw_type="icon" style:over:bg_image="end_"/>
    </view>
  </view>
  <list_view v-if="{isShowingReceipt}" name="list_view" x="0" y="0" w="320" h="210" default_item_height="60" style:normal:bg_color="#FFFFFF" disg:hide="true">
    <label name="label2" x="10" y="3" w="300" h="28" style:normal:font_size="16" text="Transaction Receipt"/>
    <scroll_view name="scroll_view" x="0" y="36" w="320" h="172">
