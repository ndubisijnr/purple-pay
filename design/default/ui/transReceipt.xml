<window v-model="transReceipt" name="transReceipt" v-on:key_down:0="{onKeyDown, Args=0}" v-on:key_down:1="{onKeyDown, Args=1}" v-on:key_down:2="{onKeyDown, Args=2}" v-on:key_down:3="{onKeyDown, Args=3}" v-on:key_down:4="{onKeyDown, Args=4}" v-on:key_down:5="{onKeyDown, Args=5}" v-on:key_down:6="{onKeyDown, Args=6}" v-on:key_down:7="{onKeyDown, Args=7}" v-on:key_down:8="{onKeyDown, Args=8}" v-on:key_down:9="{onKeyDown, Args=9}" v-on:key_down:backspace="{onKeyDown, Args=backspace}" v-on:key_down:return="{onKeyDown, Args=return}" v-on:key_down:escape="{onKeyDown, Args=escape}" v-on:key_down:delete="{onKeyDown, Args=delete}" v-on:key_down:cancel="{onKeyDown, Args=cancel}" anim_hint="htranslate" style:normal:bg_color="#FFFFFF">
  <label name="title" x="0" y="0" w="320" h="33" style:normal:text_color="#000000" style:normal:border_width="1" style:normal:font_size="18" style:normal:border="none" style:normal:round_radius="0" style:normal:border_color="#BFBFBF" text="Transaction Receipt"/>
  <list_view name="list_view" x="5" y="33" w="310" h="191" default_item_height="60">
    <scroll_view name="scroll_view" x="0" y="0" w="320" h="191">
      <view v-if="{trans.trnService == CARD_COLLECTION}" name="transaction_summary_card" h="357" style:normal:bg_color="#FFFFFF" style:normal:round_radius="5">
        <view v-if="{trans.drCr == CR}" w="310" h="35">
          <label name="tansaction_amount" x="30" y="0" w="96" h="34" style:normal:font_size="24" style:normal:text_color="#00B050" text="credit"/>
          <label name="tansaction_amount" x="120" y="0" w="126" h="34" style:normal:font_size="24" v-data:text="{amount}" style:normal:text_color="#00B050"/>
        </view>
        <view v-if="{trans.drCr == DR}" w="310" h="35">
          <label name="tansaction_amount" x="18" y="1" w="110" h="34" style:normal:font_size="24" style:normal:text_color="#C00000" text="debit"/>
          <label name="tansaction_amount" x="113" y="0" w="140" h="34" style:normal:font_size="24" v-data:text="{amount}" style:normal:text_color="#C00000"/>
        </view>
        <view name="view9" x="0" y="26" w="290" h="24" style:normal:border_color="#00000000">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Date:"/>
          <label name="fromValue" x="91" y="1" w="199" h="23" v-data:text="{trans.date}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view8" h="24" style:normal:border_color="#00000000" x="0" y="60" w="290">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Card:"/>
          <label name="fromValue" x="101" y="0" w="189" h="23" v-data:text="{extraData.card}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view7" h="24" style:normal:border_color="#00000000" x="0" y="94" w="290">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="App Label"/>
          <label name="fromValue" x="81" y="1" w="209" h="23" v-data:text="{extraData.appLab}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view6" h="24" style:normal:border_color="#00000000" x="0" y="128" w="290">
          <label name="from" x="10" y="1" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Stan"/>
          <label name="fromValue" x="65" y="1" w="225" h="23" v-data:text="{extraData.stan}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view5" h="24" style:normal:border_color="#00000000" x="0" y="196" w="290">
          <label name="from" x="10" y="0" w="46" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Status"/>
          <label name="fromValue" x="56" y="-2" w="234" h="25" v-data:text="{responseMessage}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view3" h="24" style:normal:border_color="#00000000" x="0" y="162" w="290">
          <label name="from" x="10" y="0" w="82" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Rrn"/>
          <label name="fromValue" x="65" y="1" w="225" h="23" v-data:text="{extraData.rrn}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <button v-on:click="{printNext, args=1}" name="button4" x="10" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#7B7B7B" style:normal:text_color="#FFFFFF" style:normal:border_color="#00000000" text="Merchant Copy"/>
        <button v-on:click="{onPrint}" name="button4" x="154" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#00000000" style:normal:text_color="#000000" style:normal:border_color="#7B7B7B" text="Customer Copy"/>
        <view name="view12" h="54" style:normal:border_color="#7E7E7E" x="0" y="220" w="310" style:normal:border="top" style:normal:border_width="1">
          <label name="from" x="10" y="0" w="284" h="19" style:normal:font_size="14" style:normal:text_align_h="center" text="Transaction I.D"/>
          <label name="fromValue" x="0" y="29" w="310" h="25" v-data:text="{trans.reference}" style:normal:text_align_h="center" style:normal:font_size="14"/>
        </view>
      </view>
      <view v-if="{trans.trnService != CARD_COLLECTION}" name="transaction_summary" h="357" style:normal:bg_color="#FFFFFF" style:normal:round_radius="5">
        <view v-if="{trans.drCr == CR}" w="310" h="35">
          <label name="tansaction_amount" x="32" y="0" w="96" h="34" style:normal:font_size="24" style:normal:text_color="#00B050" text="credit"/>
          <label name="tansaction_amount" x="120" y="0" w="126" h="34" style:normal:font_size="24" v-data:text="{amount}" style:normal:text_color="#00B050"/>
        </view>
        <view v-if="{trans.drCr == DR}" w="310" h="35">
          <label name="tansaction_amount" x="24" y="1" w="110" h="34" style:normal:font_size="24" style:normal:text_color="#C00000" text="debit"/>
          <label name="tansaction_amount" x="113" y="0" w="140" h="34" style:normal:font_size="24" v-data:text="{amount}" style:normal:text_color="#C00000"/>
        </view>
        <view name="view9" x="0" y="26" w="290" h="24" style:normal:border_color="#00000000">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Date:"/>
          <label name="fromValue" x="91" y="1" w="199" h="23" v-data:text="{trans.date}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view8" h="24" style:normal:border_color="#00000000" x="0" y="60" w="290">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Business Account:"/>
          <label name="fromValue" x="101" y="0" w="189" h="23" v-data:text="{trans.debitAccountNumber}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view7" h="24" style:normal:border_color="#00000000" x="0" y="94" w="290">
          <label name="from" x="10" y="0" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Account Name:"/>
          <label name="fromValue" x="81" y="1" w="209" h="23" v-data:text="{trans.creditAccountName}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view6" h="24" style:normal:border_color="#00000000" x="0" y="128" w="290">
          <label name="from" x="10" y="1" w="81" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Account Number:"/>
          <label name="fromValue" x="65" y="1" w="225" h="23" v-data:text="{trans.creditAccount}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <view name="view5" h="24" style:normal:border_color="#00000000" x="0" y="196" w="290">
          <label name="from" x="10" y="0" w="46" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Status"/>
          <label name="fromValue" x="56" y="-2" w="234" h="25" v-data:text="{responseMessage}" style:normal:text_align_h="right" style:normal:font_size="12"/>
        </view>
        <view name="view3" h="24" style:normal:border_color="#00000000" x="0" y="162" w="290">
          <label name="from" x="10" y="0" w="82" h="23" style:normal:font_size="14" style:normal:text_align_h="left" text="Bank"/>
          <label name="fromValue" x="65" y="1" w="225" h="23" v-data:text="{trans.bankName}" style:normal:text_align_h="right" style:normal:font_size="14"/>
        </view>
        <button v-on:click="{printNext, args=1}" name="button4" x="10" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#7B7B7B" style:normal:text_color="#FFFFFF" style:normal:border_color="#00000000" text="Merchant Copy"/>
        <button v-on:click="{onPrint}" name="button4" x="154" y="284" w="126" h="39" style:normal:round_radius="6" style:normal:bg_color="#00000000" style:normal:text_color="#000000" style:normal:border_color="#7B7B7B" text="Customer Copy"/>
        <view name="view12" h="54" style:normal:border_color="#7E7E7E" x="0" y="220" w="310" style:normal:border="top" style:normal:border_width="1">
          <label name="from" x="10" y="0" w="284" h="19" style:normal:font_size="14" style:normal:text_align_h="center" text="Transaction I.D"/>
          <label name="fromValue" x="0" y="29" w="310" h="25" v-data:text="{trans.reference}" style:normal:text_align_h="center" style:normal:font_size="12"/>
        </view>
      </view>
    </scroll_view>
    <scroll_bar_m name="scroll_bar" x="310" y="0" w="10" h="191" style:normal:round_radius="5" style:normal:border_width="0" style:normal:bg_color="#F4F5F8"/>
  </list_view>
</window>